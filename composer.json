{
  "name": "bedrox/bedrox",
  "license": "PHP-3.01",
  "description": "Framework indépendant développer pour une utilisation en tant qu'API. Le projet pourra renvoyer des retours dans différents langages : JSON, XML, HTML, etc...",
  "version": "0.3.1",
  "type": "library",
  "authors": [
    {
      "name": "Leslie",
      "email": "leslypetrimaux@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=7.1.3",
    "ext-pdo": "*",
    "ext-simplexml": "*",
    "ext-libxml": "*",
    "ext-json": "*",
    "ext-dom": "*",
    "ext-curl": "*"
  },
  "autoload": {
    "exclude-from-classmap": ["/docs/", "/tests/"],
    "psr-4": {
      "Bedrox\\": "src/"
    }
  }
}
